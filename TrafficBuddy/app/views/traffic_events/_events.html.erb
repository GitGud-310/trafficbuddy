<h1>All Traffic Events</h1>

<% TrafficEvent.all.each do |t| %>
    <div class="row">
        <div class="col s12">
            <div class="card blue darken-1">
                <div class="card-content white-text">
                    <span class="card-title" style="line-height: 32px;">
                        <%= t.title %>
                    </span>
                    <p><%= t.content %></p>
                </div>
            </div>
        </div>
    </div>
<% end %>




